{
  // Place your snippets for scss here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }

  // Sass-mq(from)
  "sass-mq-from-mobile": {
    "prefix": "mfm",
    "body": ["@include mq(\\$from: mobile) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-from-landscape": {
    "prefix": "mfls",
    "body": ["@include mq(\\$from: landscape) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-from-tablet": {
    "prefix": "mft",
    "body": ["@include mq(\\$from: tablet) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-from-laptop": {
    "prefix": "mflt",
    "body": ["@include mq(\\$from: laptop) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-from-desktop": {
    "prefix": "mfd",
    "body": ["@include mq(\\$from: desktop) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-from-wide": {
    "prefix": "mfw",
    "body": ["@include mq(\\$from: wide) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-from-full": {
    "prefix": "mff",
    "body": ["@include mq(\\$from: full) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  // sass-mq(until)
  "sass-mq-until-mobile": {
    "prefix": "mum",
    "body": ["@include mq(\\$until: mobile) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-until-landscape": {
    "prefix": "muls",
    "body": ["@include mq(\\$until: landscape) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-until-tablet": {
    "prefix": "mut",
    "body": ["@include mq(\\$until: tablet) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-until-laptop": {
    "prefix": "mult",
    "body": ["@include mq(\\$until: laptop) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-until-desktop": {
    "prefix": "mud",
    "body": ["@include mq(\\$until: desktop) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-until-wide": {
    "prefix": "muw",
    "body": ["@include mq(\\$until: wide) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  "sass-mq-until-full": {
    "prefix": "muf",
    "body": ["@include mq(\\$until: full) {", "\t$1", "}"],
    "description": "https://www.npmjs.com/package/@mcaskill/sass-mq"
  },

  // frontline-sass
  "fs-scale-one-property-two-viewport": {
    "prefix": "fso",
    "body": "@include fs-scale(${1:property}, (${2:min-viewport}: ${3:min-value}px, ${4:max-viewport}: ${5:max-value}px));",
    "description": "http://threespot.github.io/frontline-sass/documentation/"
  },

  "fs-scale-one-property-three-viewport": {
    "prefix": "fsot",
    "body": "@include fs-scale(${1:property}, (${2:min-viewport}: ${3:min-value}px, ${4:max-viewport}: ${5:max-value}px, ${6:max-viewport}: ${7:max-value}px));",
    "description": "http://threespot.github.io/frontline-sass/documentation/"
  },

  "fs-scale-two-property-two-viewport": {
    "prefix": "fst",
    "body": "@include fs-scale((${1:property1}, ${2:property2}), (${3:min-viewport}: ${4:min-value}px, ${5:max-viewport}: ${6:max-value}px));",
    "description": "http://threespot.github.io/frontline-sass/documentation/"
  },

  "fs-scale-two-property-three-viewport": {
    "prefix": "fstt",
    "body": "@include fs-scale((${1:property1}, ${2:property2}), (${3:min-viewport}: ${4:min-value}px, ${5:max-viewport}: ${6:max-value}px, ${7:max-viewport}: ${8:max-value}px));",
    "description": "http://threespot.github.io/frontline-sass/documentation/"
  },

  // dart-sass
  "division": {
    "prefix": "div",
    "body": "div($1, $2)",
    "description": "https://sass-lang.com/documentation/modules/math#div"
  },

  // prefer-reduced-motion
  "prefer-reduced-motion": {
    "prefix": "prm",
    "body": [
      "    @media screen and (prefers-reduced-motion: reduce) {",
      "      transition: none;",
      "    }"
    ],
    "description": "https://developer.mozilla.org/en-US/docs/Web/CSS/@media/prefers-reduced-motion"
  },

  // rem
  "postcss-rem": {
    "prefix": "rem",
    "body": "rem(${1:font-size}px)",
    "description": "https://www.npmjs.com/package/postcss-rem"
  }
}
